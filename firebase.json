{
  "functions": {
    "source": "functions",
    "runtime": "nodejs18"
  },
  "hosting": {
    "public": "web/dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/tasks",
        "function": "getWorkspaceTasks"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
